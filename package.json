	"license": "MIT",
	"repository": "https://github.com/xplato/alexandria",
	"description": "Lightweight React wrapper of `localStorage` for declarative management of user preferences and settings.",
	"keywords": ["localStorage", "settings", "typescript"],
	"main": "dist/index.js",
